        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SheetSquareActivity"></activity>
        <activity android:name=".SheetTriangleActivity" />
        <activity android:name=".SheetCircleActivity" />
        <activity android:name=".HandleActivity" />
